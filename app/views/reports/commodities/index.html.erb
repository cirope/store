<%= render 'shared/date_filter_form', url: reports_commodities_path %>

<table class="table table-hover">
  <thead>
    <tr>
      <th><%= Commodity.model_name.human count: 1 %></th>
      <th><%= ReceiptCommodity.human_attribute_name 'quantity' %></th>
    </tr>
  </thead>
  <tbody>
    <% @commodities_sales.each do |k, v| %>
      <tr>
        <td><%= k %></td>
        <td><%= v %></td>
      </tr>
    <% end %>
  </tbody>
</table>
