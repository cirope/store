<% if @warehouses.empty? %>
  <%= render 'shared/empty_index', message: t('.empty_html') %>
<% else %>
  <table class="table table-striped table-hover">
    <thead>
      <tr>
        <th><%= Warehouse.human_attribute_name 'name' %></th>
        <th colspan="3">
          <%= link_to_new new_warehouse_path, class: 'btn btn-default btn-xs pull-right' %>
        </th>
      </tr>
    </thead>

    <tbody>
      <% @warehouses.each do |warehouse| %>
        <tr>
          <td><%= warehouse.name %></td>
          <td><%= link_to_show warehouse %></td>
          <td><%= link_to_edit edit_warehouse_path(warehouse) %></td>
          <td><%= link_to_destroy warehouse %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
<% end %>

<%= link_to_new new_warehouse_path, class: 'btn btn-primary' %>
